{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/public/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/api/proxy.js"
    }
  ],
  "headers": [
    {
      "source": "/public/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
